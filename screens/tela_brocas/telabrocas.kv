<TelaBrocas>
    name: 'TelaBrocas'
    md_bg_color: app.theme_cls.primary_color

    MDToolbar:
        title: "Brocas"
        specific_text_color: [1, 1, 1, 1]
        pos_hint: {"top": 1}
        elevation: 20
        left_action_items: [["arrow-left", lambda x: root.voltar_tela_inicial()]]
        right_action_items: [["dots-vertical", lambda x: app.em_breve()]]

    

    MDBoxLayout:
        orientation: 'vertical'
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        size_hint: 0.95, 0.7
        radius: [10, 10, 10, 10]
        md_bg_color: 1, 1, 1, 1


        ScrollView:
            do_scroll_x: False

            GridLayout:
                size_hint_y: None
                cols: 1
                spacing: 15
                padding: 15


                MDRaisedButton:
                    text: "[color=#ffffff]Velocidade de Corte[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaVC'

                MDRaisedButton:
                    text: "[color=#ffffff]Velocidade do Fuso[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaVelFuso'

                MDRaisedButton:
                    text: "[color=#ffffff]Taxa de Penetração[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaTaxaPenetracao'

                MDRaisedButton:
                    text: "[color=#ffffff]Avanço por Rotação[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaAvancoRotacao'

                MDRaisedButton:
                    text: "[color=#ffffff]Taxa de Remoção de Metal[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaTaxaRemocaoMetal'

                MDRaisedButton:
                    text: "[color=#ffffff]Potência Liquida[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaPotenciaLiquida'

                MDRaisedButton:
                    text: "[color=#ffffff]Torque[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaTorque'

                MDRaisedButton:
                    text: "[color=#ffffff]Tempo de Usinagem[/color]"
                    elevation: 10
                    on_release:
                        root.manager.transition.direction = 'left'
                        root.manager.current = 'TelaTempoUsinagem'






</TelaBrocas>

<TelaVC>
    name: 'TelaVC'
    md_bg_color: app.theme_cls.primary_color

    MDToolbar:
        title: "Velocidade de Corte"
        specific_text_color: [1, 1, 1, 1]
        pos_hint: {"top": 1}
        elevation: 20
        left_action_items: [["arrow-left", lambda x: root.voltar_tela_brocas()]]
        right_action_items: [["dots-vertical", lambda x: app.em_breve()]]

    MDBoxLayout:
        orientation: 'vertical'
        pos_hint: {'center_x': 0.5, 'center_y': 0.55}
        size_hint: 0.95, 0.6
        radius: [10, 10, 10, 10]
        md_bg_color: 1, 1, 1, 1


        MDGridLayout:
            cols: 1
            spacing: sp(50)
            padding: sp(10)


            MDTextField:
                id: dm
                hint_text: "Diâmetro da Broca"
                text: ""
                required: False
                mode: "fill"
                input_filter: "float"
                input_type: 'number'
                radius: 10, 10, 10, 10
                on_text: root.calcular()

            MDTextField:
                id: ve
                hint_text: "Velocidade do eixo"
                text: ""
                required: False
                mode: "fill"
                input_filter: "float"
                input_type: 'number'
                radius: 10, 10, 10, 10
                on_text: root.calcular()

            MDLabel:
                id: resultado
                text: "0.00 m/min"
                font_style: "H4"
                halign: "center"



</TelaVC>

